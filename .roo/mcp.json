{
  "mcpServers": {
    "fetch": {
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ],
      "alwaysAllow": [
        "fetch"
      ]
    },
    "Context7": {
      "disabled": true,
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ],
      "alwaysAllow": [
        "get-library-docs"
      ]
    }
  }
}